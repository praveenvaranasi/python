        <Appender AppenderName="Syslog4jB2BOutAppender" AppenderType="syslog" LoggingEnabled="false"
                  Hostname="localhost" Protocol="udp" Port="514" Facility="local7"
		          LogPattern="!ENTRY %n[%d{dd/MMM/yyyy HH:mm:ss}] %-5p [%c{1}] %m%n"
