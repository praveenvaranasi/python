                   EnsureUniquenessOfClientId="true" LazyThreadCreation="true"
                   ObjectName="Fiorano.etc:ServiceType=FMQConfigLoader,Name=FMQConfigLoader,type=config"
                   PingEnabled="true" UpdateConnectURL="false"/>
